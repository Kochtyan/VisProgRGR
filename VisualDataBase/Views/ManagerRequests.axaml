<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="600" d:DesignHeight="400"
        x:Class="VisualDataBase.Views.ManagerRequests"
        Title="ManagerRequests"
        Width="600" Height="400">

	<Window.Styles>
		<Style Selector="Button.button2">
			<Setter Property="HorizontalContentAlignment" Value="Center"/>
			<Setter Property="FontSize" Value="18"/>
			<Setter Property="Width" Value="50"/>
			<Setter Property="Height" Value="30"/>
			<Setter Property="Margin" Value="5"/>
			<Setter Property="Background" Value="#17376D"/>
		</Style>
	</Window.Styles>

  <Grid RowDefinitions="auto *" Background="#13685C">
    <WrapPanel Grid.Row="0">
      <Button Classes="button2" Content="V"/>
      <Button Classes="button2" Content="X"/>
      <Button Classes="button2" Content="B"/>
      <Button Classes="button2" Content="C"/>
      <Button Classes="button2" Content="G"/>
      <Button Classes="button2" Content="P"/>
    </WrapPanel>
  </Grid>
  
</Window>
